# Custom iterator class

class MyRange:
    def __init__(self, start, end):
        self.current = start
        self.end = end
        
    def __iter__(self):
        return self
    
    def __next__(self):
        if self.current >= self.end:
            raise StopIteration
            
        else:
            self.current += 2
            return self.current - 1
    
