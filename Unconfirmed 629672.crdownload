# In[103]:


def nthEvenFibonacci(n):
    MOD = 1000000007

    # Base cases
    if n == 1:
        return 2
    if n == 2:
        return 8

    # Iterative computation of even Fibonacci numbers
    prev2, prev1 = 2, 8  # E_1 = 2, E_2 = 8

    for _ in range(3, n + 1):
        current = (4 * prev1 + prev2) % MOD
        prev2, prev1 = prev1, current

    return prev1

# Example Usage
