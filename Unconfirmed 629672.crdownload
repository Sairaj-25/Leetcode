# 22 Binary Search Algorithm

# Binary Search in Sorted list

def binary_search(arr, target):
    low = 0
    high = len(arr) - 1
    
    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == target:
            return mid
        elif arr[mid] < target:
            low = mid+1
        else:
            high = mid -1
    return -1

arr = [1, 3, 5, 7, 9]
print(binary_search(arr, 7))


