def countFibonacciNumbers(arr):
        n = max(arr)
        fib = set()
        a = 0
        b = 1
        counter = 0
        while a <= n:
            fib.add(a)
            a, b = b, a+b
        
        for i in arr:
            if i in fib:
                counter+=1
        
        return counter
